n  }\n  const [state, setState] = useState(initialState);\n  // --- ここまで追加 ---\n\n  useEffect(() => {\n    dispatch({ type: foodsActionTyps.FETCHING });\n    fetchFoods(match.params.restaurantsId)\n      .then((data) => {\n        dispatch({\n          type: foodsActionTyps.FETCH_SUCCESS,\n          payload: {\n            foods: data.foods\n          }\n        });\n      })\n  }, []);\n\n  return (\n    <Fragment>\n      <HeaderWrapper>\n        <Link to=\"/restaurants\">\n          <MainLogoImage src={MainLogo} alt=\"main logo\" />\n        </Link>\n        <BagIconWrapper>\n          <Link to=\"/orders\">\n            <ColoredBagIcon fontSize=\"large\" />\n          </Link>\n        </BagIconWrapper>\n      </HeaderWrapper>\n      <FoodsList>\n        {\n          foodsState.fetchState === REQUEST_STATE.LOADING ?\n            <Fragment>\n              {\n                [...Array(12).keys()].map(i =>\n                  <ItemWrapper key={i}>\n                    <Skeleton key={i} variant=\"rect\" width={450} height={180} />\n                  </ItemWrapper>\n                )\n              }\n            </Fragment>\n          :\n            foodsState.foodsList.map(food =>\n              <ItemWrapper key={food.id}>\n                <FoodWrapper\n                  food={food}\n                  // フードitemクリック時にsetStateする\n                  onClickFoodWrapper={(food) => setState({\n                    ...state,\n                    selectedFood: food,\n                    isOpenOrderDialog: true,\n                  })}\n                  imageUrl={FoodImage}\n                />\n              </ItemWrapper>\n            )\n        }\n      </FoodsList>\n      {\n        state.isOpenOrderDialog &&\n          <FoodOrderDialog\n            food={state.selectedFood}\n            isOpen={state.isOpenOrderDialog}\n            onClose={() => setState({\n              ...state,\n              isOpenOrderDialog: false,\n            })}\n          />\n      }\n    </Fragment>\n  )\n}\n","/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/containers/Orders.jsx",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/apis/restaurants.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/urls/index.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/reducers/restaurants.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/constants.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/apis/foods.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/reducers/foods.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/style_constants.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/components/FoodWrapper.jsx",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/components/StyledText.jsx",[],["81","82"],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/components/Icons/index.js",[],"/Users/ko-suke24/Desktop/uber-eats-like/frontend/src/components/FoodOrderDialog.jsx",["83"],"// --- ここから追加 ---\nimport React from 'react';\nimport { DialogContent, Dialog, DialogTitle, DialogActions } from '@material-ui/core';\nimport styled from 'styled-components';\n\n// components\nimport { SubText } from './StyledText';\n\n// images\nimport OrderHeaderImage from '../images/order-header.png';\n\nconst OrderHeader = styled.img`\n  width: 100%;\n  height: 350px;\n`;\n\nconst DescriptionWrapper = styled.div`\n  padding: 0 8px 8px 8px;\n  height: 50px;\n`;\n\nexport const FoodOrderDialog = ({\n  food,\n  isOpen,\n  onClose,\n}) => {\n  return (\n    <Dialog\n      open={isOpen}\n      onClose={onClose}\n    >\n      <OrderHeader src={OrderHeaderImage} alt=\"order header\" />\n      <DialogTitle>\n        {food.name}\n      </DialogTitle>\n      <DialogContent>\n        <DescriptionWrapper>\n          <SubText>\n            {food.description}\n          </SubText>\n        </DescriptionWrapper>\n      </DialogContent>\n      <DialogActions>\n       // 数量を操作するアクションを入れる予定\n      </DialogActions>\n 